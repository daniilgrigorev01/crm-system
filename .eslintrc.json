{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": ["standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
  }
}
